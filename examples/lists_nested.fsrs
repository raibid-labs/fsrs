// Nested lists
// Demonstrates lists of lists and complex structures

// Matrix representation using nested lists
let matrix = [[1; 2]; [3; 4]; [5; 6]] in

// List of empty lists
let empty_nested = [[]; []; []] in

// Mixed nested lists with cons construction
let mixed = [1 :: [2]; []; [3; 4; 5]] in

// Different nesting levels
let deep = [[[1]]; [[2; 3]]; [[4; 5; 6]]] in

// Return matrix for display
// Expected output: [[1; 2]; [3; 4]; [5; 6]]
matrix
